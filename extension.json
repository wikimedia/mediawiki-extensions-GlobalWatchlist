{
	"name": "GlobalWatchlist",
	"author": [
		"DannyS712"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:GlobalWatchlist",
	"descriptionmsg": "globalwatchlist-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"SpecialPages": {
		"GlobalWatchlist": {
			"class": "MediaWiki\\Extension\\GlobalWatchlist\\SpecialGlobalWatchlist",
			"factory": "MediaWiki\\Extension\\GlobalWatchlist\\SpecialGlobalWatchlist::newFromGlobalState",
			"services": [
				"StatsdDataFactory"
			]
		},
		"GlobalWatchlistSettings": "MediaWiki\\Extension\\GlobalWatchlist\\SpecialGlobalWatchlistSettings"
	},
	"HookHandlers": {
		"GlobalWatchlistHooks": {
			"class": "MediaWiki\\Extension\\GlobalWatchlist\\GlobalWatchlistHooks",
			"services": [
				"SpecialPageFactory"
			]
		}
	},
	"Hooks": {
		"GetPreferences": {
			"handler": "GlobalWatchlistHooks"
		},
		"LoginFormValidErrorMessages": {
			"handler": "GlobalWatchlistHooks"
		},
		"SkinBuildSidebar": {
			"handler": "GlobalWatchlistHooks"
		}
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\GlobalWatchlist\\": "includes/"
	},
	"MessagesDirs": {
		"GlobalWatchlist": [
			"i18n",
			"i18n/api",
			"i18n/script"
		]
	},
	"ExtensionMessagesFiles": {
		"GlobalWatchlistAliases": "GlobalWatchlist.alias.php"
	},
	"APIModules": {
		"globalwatchlistsettings": {
			"class": "MediaWiki\\Extension\\GlobalWatchlist\\ApiGlobalWatchlistSettings",
			"factory": "MediaWiki\\Extension\\GlobalWatchlist\\ApiGlobalWatchlistSettings::newFromGlobalState",
			"services": [
				"GlobalWatchlistSettingsManager"
			]
		}
	},
	"ServiceWiringFiles": [
		"includes/ServiceWiring.php"
	],
	"config": {
		"GlobalWatchlistWikibaseSite": {
			"value": "www.wikidata.org",
			"description": "Site for which labels should be fetched for items on the watchlist"
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "GlobalWatchlist/modules"
	},
	"ResourceModules": {
		"ext.globalwatchlist.specialglobalwatchlist": {
			"packageFiles": [
				"ext.globalwatchlist.specialglobalwatchlist.js",
				"ext.globalwatchlist.debug.js",
				"ext.globalwatchlist.getSettings.js",
				"ext.globalwatchlist.linker.js",
				"ext.globalwatchlist.notifications.js",
				"ext.globalwatchlist.site.js",
				"ext.globalwatchlist.watchlistUtils.js"
			],
			"styles": [
				"ext.globalwatchlist.common.css",
				"ext.globalwatchlist.specialglobalwatchlist.css"
			],
			"messages": [
				"globalwatchlist-asof",
