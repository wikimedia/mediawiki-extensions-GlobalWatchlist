				"oojs-ui-core",
				"oojs-ui-widgets",
				"oojs-ui-windows",
				"oojs-ui.styles.icons-content",
				"oojs-ui.styles.icons-interactions",
				"oojs-ui.styles.icons-media",
				"oojs-ui.styles.icons-moderation",
				"oojs-ui.styles.icons-movement",
				"user.options"
			]
		},
